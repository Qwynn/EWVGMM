			<harvestWork>650</harvestWork>
			<harvestYield>30</harvestYield>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<harvestedThingDef>WoodPoplar</harvestedThingDef>
		</plant>
	</ThingDef>
	
	<ThingDef ParentName="VG_TreewoodBase">
